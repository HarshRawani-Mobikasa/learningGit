            <div class=" tags-img-1 mt-md-5"><img src="https://cdn.shopify.com/s/files/1/0619/0039/0599/files/Frame_5.svg?v=1669614038" alt="tag"></div>
            <div class=" tags-img-1 mt-md-5 "><img src="https://cdn.shopify.com/s/files/1/0619/0039/0599/files/Group_62.svg?v=1669614051" alt="tag"></div>
          </div>
        </div>
        <div class="bttn">
          <a href="{{ section.settings.view_all_link }}"> <button>{{ section.settings.view_all_text }}</button></a>
         
        </div>
      </div>
    </section>





