<section id="Explore-More">
        <div class="container-fluid">
          <div class="section-title">
            <h2>Explore More</h2>
          </div>
          <div class="row">
            <div class="col-lg-12 d-flex flex-column justify-content-center pt-lg-0 order-2 order-lg-1 aos-init aos-animate" data-aos="fade-up" data-aos-delay="200">
              <div class="explore-items">
                {% for block in section.blocks %}
                <div class="item">
                  <img src="{{ block.settings.explore_img | img_url:'medium' }}">
                  <p>{{ block.settings.heading }}</p>
                </div>
                {% endfor %}
                </div>
                <div class="viewAllBtn">
                  <button>view all</button>
                </div>
              </div>
            </div>
          </div>
        
      </section>

{% schema %}
  {
      "name": "Explore More Section",
      
      "blocks": [
        {
          "type": "text",
          "name": "slide",
          "settings": [
          {
            "type": "image_picker",
            "id": "explore_img",
            "label": "Explore Image"
          },
          {
            "type": "text",
            "id": "heading",
            "label": "Explore Text"
          } 
     
          ]
        }
      ],
      "presets": [
        {
          "name": "Explore More Section",
          "category": "text",
          "blocks": [
            {
              "type": "text"
            }
          ]
        }
      ]
    }
{% endschema %}

